{
  "name": "@tanstack/angular-table",
  "version": "8.11.7",
  "description": "Headless UI for building powerful tables & datagrids for Angular.",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/table.git",
    "directory": "packages/angular-table"
  },
  "homepage": "https://tanstack.com/table",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "keywords": [
    "angular",
    "table",
    "angular-table",
    "datagrid"
  ],
  "type": "module",
  "module": "fesm2022/tanstack-angular-table.mjs",
  "main": "index.d.ts",
  "types": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "build/lib/*",
    "build/umd/*",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./build",
    "test:types": "tsc --noEmit",
    "build": "pnpm ng-packagr -p ng-package.json",
    "build:rollup": "rollup --config rollup.config.mjs",
    "build:types": "tsc --emitDeclarationOnly"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@tanstack/table-core": "workspace:*"
  },
  "peerDependencies": {
    "@angular/core": "^17.1.2",
    "@angular/common": "^17.1.2"
  },
  "sideEffects": false
}
