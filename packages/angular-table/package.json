{
  "name": "@tanstack/angular-table",
  "version": "8.17.3",
  "description": "Headless UI for building powerful tables & datagrids for Angular.",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/table.git",
    "directory": "packages/angular-table"
  },
  "homepage": "https://tanstack.com/table",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "keywords": [
    "angular",
    "table",
    "angular-table",
    "datagrid"
  ],
  "type": "module",
  "module": "dist/esm2022/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "esm": "./dist/esm2022/index.mjs",
      "esm2022": "./dist/esm2022/index.mjs",
      "default": "./dist/fesm2022/tanstack-angular-table.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test:types": "tsc",
    "test:build": "publint --strict",
    "build": "pnpm ng-packagr -p ng-package.json && rimraf ./dist/package.json"
  },
  "dependencies": {
    "@tanstack/table-core": "workspace:*",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@angular/core": "^17.3.1",
    "ng-packagr": "^17.3.0"
  },
  "peerDependencies": {
    "@angular/core": ">=17"
  },
  "sideEffects": false
}
